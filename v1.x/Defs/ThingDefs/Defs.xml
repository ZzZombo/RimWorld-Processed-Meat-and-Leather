<Defs>
	<ThingDef ParentName="LeatherBase">
		<defName>Leather_Processed</defName>
		<label>processed leather</label>
		<description>A much stonger and beautiful version of patchleather created by cutting up and sewing together several pieces of it. It also provides much better insulation.</description>
		<graphicData>
			<color>(45,34,30)</color>
		</graphicData>
		<statBases>
			<Beauty>1</Beauty>
			<MarketValue>3.4</MarketValue>
			<StuffPower_Armor_Sharp>1.12</StuffPower_Armor_Sharp>
			<StuffPower_Insulation_Cold>30</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>30</StuffPower_Insulation_Heat>
		</statBases>
		<stuffProps>
			<color>(90,75,60)</color>
			<commonality>0.05</commonality>
			<statFactors>
				<Beauty>1</Beauty>
			</statFactors>
		</stuffProps>
	</ThingDef>
