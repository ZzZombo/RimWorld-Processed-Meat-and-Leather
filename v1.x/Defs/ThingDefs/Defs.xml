	<ThingDef ParentName="ResourceBase">
		<defName>Meat_Processed</defName>
		<label>processed meat</label>
		<description>Processed from several other meat pieces meat that has no distinct taste carried over from them.</description>
		<graphicData>
			<color>(90,34,30)</color>
			<graphicClass>Graphic_StackCount</graphicClass>
			<texPath>Things/Item/Resource/MeatFoodRaw/Meat_Small</texPath>
		</graphicData>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<Beauty>-4</Beauty>
			<DeteriorationRate>6</DeteriorationRate>
			<Mass>0.03</Mass>
			<Flammability>0.5</Flammability>
			<Nutrition>0.05</Nutrition>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
			<MarketValue>2</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>2</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<socialPropernessMatters>true</socialPropernessMatters>
		<healthAffectsPrice>false</healthAffectsPrice>
		<thingCategories>
			<li>MeatRaw</li>
		</thingCategories>
		<ingestible>
			<foodType>Meat</foodType>
			<preferability>RawBad</preferability>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<tasteThought>AteRawFood</tasteThought>
		</ingestible>
	</ThingDef>
	<RecipeDef>
		<defName>Make_ProcessedMeat</defName>
		<label>make processed meat</label>
		<description>Combine pieces of meat to create a neutral version of it.</description>
		<jobString>Making processed meat.</jobString>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<effectWorking>ButcherFlesh</effectWorking>
		<interruptIfIngredientIsRotting>true</interruptIfIngredientIsRotting>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<workAmount>150</workAmount>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Processed</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<Meat_Processed>10</Meat_Processed>
		</products>
		<workSkill>Cooking</workSkill>
	</RecipeDef>
